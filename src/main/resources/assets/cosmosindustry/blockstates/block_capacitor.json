{
    "multipart": [
        {   "apply": { "model": "cosmosindustry:block/capacitor/base" }},
        
        {   "when": { "north": "0" },
            "apply": { "model": "cosmosindustry:block/capacitor/side", "uvlock": true }
        },
        {   "when": { "east": "0" },
            "apply": { "model": "cosmosindustry:block/capacitor/side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "0" },
            "apply": { "model": "cosmosindustry:block/capacitor/side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "0" },
            "apply": { "model": "cosmosindustry:block/capacitor/side", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "0" },
            "apply": { "model": "cosmosindustry:block/capacitor/side", "x": 270, "uvlock": true }
        },
        {   "when": { "down": "0" },
            "apply": { "model": "cosmosindustry:block/capacitor/side", "x": 90, "uvlock": true }
        },
        
        
        {   "when": { "north": "1" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_output", "uvlock": true }
        },
        {   "when": { "east": "1" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_output", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "1" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_output", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "1" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_output", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "1" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_output", "x": 270, "uvlock": true }
        },
        {   "when": { "down": "1" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_output", "x": 90, "uvlock": true }
        },
        
        
        {   "when": { "north": "2" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_input", "uvlock": true }
        },
        {   "when": { "east": "2" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_input", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "2" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_input", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "2" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_input", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "2" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_input", "x": 270, "uvlock": true }
        },
        {   "when": { "down": "2" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_input", "x": 90, "uvlock": true }
        },
        
        
        {   "when": { "north": "3" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_disabled", "uvlock": true }
        },
        {   "when": { "east": "3" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_disabled", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "3" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_disabled", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "3" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_disabled", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "3" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_disabled", "x": 270, "uvlock": true }
        },
        {   "when": { "down": "3" },
            "apply": { "model": "cosmosindustry:block/capacitor/side_disabled", "x": 90, "uvlock": true }
        }
    ]
}